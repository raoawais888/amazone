<%- include('../partials/header.ejs')%>

<section id="breadcrumb">

  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <h2>Reset Your Password</h2>
      </div>
    </div>
  </div>

</section>

<div class="container py-5">

  <div class="row">
    <div class="col-md-5 mx-auto login_box"> 
      
  
<form method="post">


     
    <% if(message.success){ %>
    <div class="alert alert-success"><%= message.success %></div>
    <% }else if(message.fail) { %>
    <div class="alert alert-danger"><%= message.fail %></div>
    <% } %>

      <% if(typeof message_show != "undefined") {%>
        <div class="alert alert-danger"><%= message_show%></div>

      <% } %>
    <h1>Reset Your Password</h1>
    <label for="email"><b>Password</b></label>
    <input type="password" class="form-control" placeholder="Enter New Password" name="password" id="password" />

    <button type="submit" class="log_btn d-block w-100 action_btn">Reset Your Password</button> 
     
      
    </div>
    
</form>
</div>
</div>
</div>

<%- include('../partials/footer.ejs'); %>
